#######################################################################
# n8n configuration
#######################################################################

# 64-byte hex recommended; keep private
N8N_ENCRYPTION_KEY=CHANGE_ME_64_HEX
# Long random string for user management JWT
N8N_USER_MANAGEMENT_JWT_SECRET=CHANGE_ME_LONG_RANDOM

# The hostname used for n8n (Cloudflare tunnel + WEBHOOK_URL)
N8N_HOSTNAME=n8n.example.com
N8N_PROTOCOL=https
WEBHOOK_URL=https://n8n.example.com

# n8nâ€™s internal container port
N8N_PORT=5678
# Set false if TLS terminates at proxy/tunnel (e.g., Cloudflared/Caddy)
N8N_SECURE_COOKIE=false


#######################################################################
# Supabase configuration
#######################################################################

POSTGRES_PASSWORD=CHANGE_ME_STRONG_DB_PASSWORD
JWT_SECRET=CHANGE_ME_LONG_RANDOM
ANON_KEY=CHANGE_ME_SUPABASE_ANON_KEY
SERVICE_ROLE_KEY=CHANGE_ME_SUPABASE_SERVICE_ROLE_KEY
DASHBOARD_USERNAME=supabase
DASHBOARD_PASSWORD=CHANGE_ME_DASHBOARD_PASSWORD
POOLER_TENANT_ID=1000


#######################################################################
# Neo4j configuration
#######################################################################

# Format: username/password
NEO4J_AUTH=neo4j/CHANGE_ME_NEO4J_PASSWORD
NEO4J_USER=neo4j
NEO4J_PASSWORD=CHANGE_ME_NEO4J_PASSWORD


#######################################################################
# Langfuse / Observability (and related services)
#######################################################################

CLICKHOUSE_PASSWORD=CHANGE_ME_CLICKHOUSE_PASSWORD
MINIO_ROOT_PASSWORD=CHANGE_ME_MINIO_ROOT_PASSWORD
LANGFUSE_SALT=CHANGE_ME_LONG_RANDOM
NEXTAUTH_SECRET=CHANGE_ME_LONG_RANDOM
ENCRYPTION_KEY=CHANGE_ME_64_HEX
# Note: Host name may be 'postgres' or 'db' depending on your compose file
DATABASE_URL=postgresql://postgres:${POSTGRES_PASSWORD}@postgres:5432/postgres
CLICKHOUSE_URL=http://clickhouse:${CLICKHOUSE_PASSWORD}@clickhouse:8123/default
CLICKHOUSE_MIGRATION_URL=clickhouse://${CLICKHOUSE_PASSWORD}@clickhouse:9000/default

#######################################################################
# Database defaults
#######################################################################

POSTGRES_HOST=db
POSTGRES_DB=postgres
POSTGRES_PORT=5432
POSTGRES_USER=postgres


#######################################################################
# Supavisor / connection pooler
#######################################################################

POOLER_PROXY_PORT_TRANSACTION=6543
POOLER_DEFAULT_POOL_SIZE=20
POOLER_MAX_CLIENT_CONN=100
SECRET_KEY_BASE=CHANGE_ME_LONG_RANDOM
# Must be exactly 32 characters
VAULT_ENC_KEY=CHANGE_ME_32_CHAR_ENC_KEY________________
POOLER_DB_POOL_SIZE=5


#######################################################################
# Misc API ports (internal defaults)
#######################################################################

KONG_HTTP_PORT=8000
KONG_HTTPS_PORT=8443
PGRST_DB_SCHEMAS=public,storage,graphql_public


#######################################################################
# Flowise auth
#######################################################################

FLOWISE_USERNAME=you@example.com
FLOWISE_PASSWORD=CHANGE_ME_FLOWISE_PASSWORD


#######################################################################
